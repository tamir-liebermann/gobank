{
  "version": 2,
  "builds": [
    {
      "src": "/api(.*)",
      "use": "@vercel/go"
    }
],
    "routes": [
    {
      "src": "/ginapi",
      "dest": "ginapi.go"
    },

     {
      "src": "/gptapi",
      "dest": "gptapi.go"
     },
     {
        "src": "/main",
        "dest": "main.go"
     }

    
  ]

}
