{
  "version": 2,
  "builds": [
    {
      "src": "main.go",
      "use": "@vercel/go"
    }
  ],
  "routes": [
    {
      "src": "/ginapi(.*)",
      "dest": "/main"
    },
    {
      "src": "/gptapi(.*)",
      "dest": "/main"
    },
    {
      "src": "/(.*)",
      "dest": "/main"
    }
  ]
}
